<% provide(:title, 'Users') %>
<h1>All users</h1>

<ul class="users">
  <% @users.each do |user| %>
    <%= render user %>
  <% end %>
</ul>

<!-- The will_paginate method is a little magical; inside a users view, it
automatically looks for an @users object, and then displays pagination links
to access other pages -->
<%= will_paginate @users %>
